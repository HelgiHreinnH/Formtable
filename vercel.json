{
  "version": 2,
  "name": "form-desk",
  "public": true,
  "github": {
    "silent": true
  },
  "redirects": [
    {
      "source": "/wireframes",
      "destination": "/design/wireframes/form-desk-optimized.html"
    },
    {
      "source": "/demo",
      "destination": "/design/wireframes/form-desk-optimized.html"
    },
    {
      "source": "/design-system",
      "destination": "/design/wireframes/form_desk_wireframe_visual.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
}